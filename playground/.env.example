# Nuxt Runtime Configuration for tlv2-ui
# Copy this file to .env and customize for your local environment

# ============================================
# Server-side Configuration (Private)
# ============================================

# GraphQL API key for unauthenticated requests (server-side only)
# NUXT_TLV2_GRAPHQL_APIKEY=

# Server-side Transitland API proxy base (not exposed to browser)
# NUXT_TLV2_PROXY_BASE_DEFAULT=http://localhost:8080
# NUXT_TLV2_PROXY_BASE_STATION_EDITOR=http://localhost:8080
# NUXT_TLV2_PROXY_BASE_FEED_MANAGEMENT=http://localhost:8080

# ============================================
# Public Configuration (Client & Server)
# ============================================

# Proxy settings
# NUXT_PUBLIC_TLV2_USE_PROXY=false

# Public API endpoints (automatically set when proxy is enabled)
# NUXT_PUBLIC_TLV2_API_BASE_DEFAULT=http://localhost:8080
# NUXT_PUBLIC_TLV2_API_BASE_STATION_EDITOR=http://localhost:8080
# NUXT_PUBLIC_TLV2_API_BASE_FEED_MANAGEMENT=http://localhost:8080

# Map tile API keys
# NUXT_PUBLIC_TLV2_PROTOMAPS_APIKEY=
# NUXT_PUBLIC_TLV2_NEARMAPS_APIKEY=

# Link configuration
# NUXT_PUBLIC_TLV2_SAFELINK_UTM_SOURCE=

# Editor route prefix
# NUXT_PUBLIC_TLV2_EDITOR_ROUTE_PREFIX=editor

# Login gate settings
# NUXT_PUBLIC_TLV2_LOGIN_GATE=false
# NUXT_PUBLIC_TLV2_REQUIRE_LOGIN=false

# Auth0 configuration
# NUXT_PUBLIC_TLV2_AUTH0_DOMAIN=
# NUXT_PUBLIC_TLV2_AUTH0_CLIENT_ID=
# NUXT_PUBLIC_TLV2_AUTH0_AUDIENCE=
# NUXT_PUBLIC_TLV2_AUTH0_SCOPE=
# NUXT_PUBLIC_TLV2_AUTH0_REDIRECT_URI=
# NUXT_PUBLIC_TLV2_AUTH0_LOGOUT_URI=
