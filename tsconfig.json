{
  // https://v3.nuxtjs.org/concepts/typescript
  // "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "target": "esnext",
    "module": "esnext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "@nuxt/types",
      "@types/node"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    ".output",
    ".nuxt"
  ],
  "include": [
    "src/**/*"
  ]
}